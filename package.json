  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fogfish/scud"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "*",
    "@aws-cdk/aws-cognito": "*",
    "@aws-cdk/aws-lambda": "*",
    "@aws-cdk/aws-logs": "*",
    "@aws-cdk/core": "*",
    "@types/node": "15.0.2",
    "aws-cdk-pure": "^1.3.19"
  },
  "devDependencies": {
    "@aws-cdk/assert": "*",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
