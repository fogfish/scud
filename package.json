  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fogfish/scud"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "*",
    "@aws-cdk/aws-cognito": "*",
    "@aws-cdk/aws-lambda": "*",
    "@aws-cdk/aws-logs": "*",
    "@aws-cdk/aws-route53": "*",
    "@aws-cdk/aws-route53-targets": "*",
    "@aws-cdk/core": "*",
    "@types/node": "16.6.1",
    "aws-cdk-pure": "^1.3.20"
  },
  "devDependencies": {
    "@aws-cdk/assert": "*",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "coveralls": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  }
}
