{
  "name": "aws-scud",
  "version": "0.2.2",
  "description": "scud - simple cloud usable daemon",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint lib/*.ts",
    "test": "GO111MODULE=off jest --no-cache --coverage",
