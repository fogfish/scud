{
  "name": "aws-scud",
  "version": "0.3.0",
  "description": "scud - simple cloud usable daemon",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint lib/*.ts",
    "test": "GO111MODULE=off jest --no-cache --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "clean": "rm lib/*.js lib/*.d.ts"
